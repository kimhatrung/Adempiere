  <AD_WINDOW_TRL AD_WINDOW_ID="153" AD_LANGUAGE="es_MX" AD_CLIENT_ID="0" AD_ORG_ID="0" ISACTIVE="Y" CREATED="2006-11-10 00:02:24.0" CREATEDBY="100" UPDATED="2011-08-25 09:14:49.0" UPDATEDBY="100" NAME="Combinación de Cuentas" DESCRIPTION="Mantener combinaciones de cuenta válida" HELP="Define y despliega combinaciones contables válidas." ISTRANSLATED="Y" />
